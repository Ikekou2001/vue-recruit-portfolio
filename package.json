    "vue",
    "azure-functions",
    "typescript",
    "portfolio",
    "recruitment"
  ],
  "author": "plumiume.com",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.22.0",
  "dependencies": {
    "create-vite": "^8.2.0"
  },
  "pnpm": {
    "overrides": {
      "rollup": "4.32.1"
    }
  }
}
